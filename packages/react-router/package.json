{
  "name": "@better-pages-generate/react-router",
  "version": "1.0.0",
  "description": "Generated react-router file-based routes for Vite",
  "type": "module",
  "author": "Chiko",
  "keywords": [
    "react-router",
    "file-base",
    "pages",
    "file-based-routing",
    "typescript",
    "vite"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --clean",
    "type-check": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./lazy": {
      "types": "./dist/index-lazy.d.ts",
      "import": "./dist/index-lazy.js"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "require": "./dist/plugin/index.cjs",
      "import": "./dist/plugin/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "core": [
        "./dist/core.d.ts"
      ],
      "lazy": [
        "./dist/index-lazy.d.ts"
      ],
      "plugin": [
        "./dist/plugin/index.d.ts"
      ]
    }
  },
  "files": [
    "src"
  ],
  "packageManager": "pnpm@10.13.0",
  "devDependencies": {
    "@types/react": "^19.1.8",
    "react-router": "^7.7.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-router": ">=7",
    "vite": ">=5"
  },
  "dependencies": {
    "better-pages-generate": "workspace:*",
    "fast-glob": "^3.3.3"
  }
}