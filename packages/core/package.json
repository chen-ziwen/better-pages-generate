{
  "name": "better-pages-generate",
  "version": "1.0.0",
  "description": "Better file routing library based on react-router",
  "main": "index.js",
  "type": "module",
  "author": "Chiko",
  "keywords": [
    "react-router",
    "file-base",
    "pages",
    "file-based-routing",
    "typescript",
    "vite"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vitest watch",
    "build": "tsup --clean",
    "test": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "exports": {
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    }
  },
  "typesVersions": {
    "*": {
      "core": [
        "./src/core.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@10.13.0",
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "vite": ">=5",
    "react": ">=18",
    "react-router": ">=7"
  }
}
